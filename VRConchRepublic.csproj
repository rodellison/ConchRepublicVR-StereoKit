  <ItemGroup>
    <Compile Update="ConchRepublicStrings.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>ConchRepublicStrings.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="ConchRepublicStrings.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>ConchRepublicStrings.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

